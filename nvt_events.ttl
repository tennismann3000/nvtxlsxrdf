nvt:ev_clownonade_001 a nvto:Event ;
    nvto:happenedAtPlace nvt:berlin_ost,
        nvt:ddr,
        nvt:loc_dtks_foyer ;
    nvto:hasMusician nvt:dietrich_petzold ;
    nvto:hasPerformer nvt:anja_palitzsch,
        nvt:bernd_hahnke,
        nvt:burkhart_seidemann,
        nvt:christoph_posselt,
        nvt:gisela_bohmann,
        nvt:guenter_richter,
        nvt:hans_otto_reintsch,
        nvt:peter_baumgart ;
    nvto:hasSeason nvt:timespan_ev_clownonade_001_1988 ;
    dcterms:contributor nvt:g_dt_pe ;
    dcterms:date "1988-12-04" ;
    dcterms:isPartOf nvt:ev_theater_in_bewegung_1988_001,
        nvt:pr_clownonade ;
    dcterms:source "Beschreibung: Pantomimisches Spektakel für Kinder zwishen Sieben und Siebzig. Ein turbulentes Spiel, geboren aus den komischen Einfällen einer ihrer Langeweile überdrüssigen Clown-Gesellschaft. Drehungen und Wendungen einer blühenden Phantasie, die aus dem Nichts die Gefahren einer Raubtierdressur, einer verrückten Seereise, eines Schiffbruchs oder die haarsträubenden Abenteuer einer Dschungeltour zaubert und dabei immer wieder das Publikum zum Mitmachen anstiftet. Ein unterhaltsamer Spaß für jeden, der seine Kindheit nicht vergessen hat. Quelle: CW; txt-007-001" ;
    dcterms:type nvt:auffuehrung ;
    skos:prefLabel "Clownonade" .

